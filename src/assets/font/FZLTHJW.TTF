<script type="text/javascript">
	function openChat(){
			var path="/aftersales/c/aftersales/webchat?productId=&typeId=";
			window.open(path,"_blank");
	}
	$(function(){
		var widthInner = window.innerWidth;
		if(widthInner<600){
			rightClick();
		}
		$('.newcall').click(function(){
			$('.phoneN').animate({right:'1.01%'});
		});
		$('.phoneN a').click(function(){
			$(this).parent('.phoneN').animate({right:'-167px'});
		});
		
		$(document).scroll(function(){
	        var scroll=$(document).scrollTop();
	        if(scroll>0){
	    		$('.head').addClass('navbar-fixed');
	    		$('body').addClass('head-fixed');
	    	}else{
	    		$('.head').removeClass('navbar-fixed');
	    		$('body').removeClass('head-fixed');
	    	}
	    });
		$('.bgblack li a.wall').click(function(){
			$('.chatCall').animate({'right':'0px'},400);
		});
		$('#chatclose').click(function(){
			$(this).parents('.chatCall').animate({'right':'-343px'},400);
		});
	})
	function rightClick(){
		$(".side_bar .m_side").click(function(event){
			$(this).hide();
			$(this).next('.btn_box').show();
			$(this).parent('.side_bar').css("top","19%");
			event.stopPropagation();
		});
		$('body').click(function(event){
			$(".side_bar .btn_box").hide();
			$(".side_bar .m_side").show();
			$('.side_bar').css("top","35%");
		});
	}
</script>
	
