<script type="text/javascript">
	$(function(){
		if(""){
			stm_clicki('set','dimension1','');
		}
		var slide = $('.mob_nav');
		var slideLi = $('.mob_nav ul li:nth-child(0)');
		$('.modle_btn').click(function(){
			if(slide.hasClass('slideHid')){
				slide.slideDown();
				slide.removeClass('slideHid');
				
			}else{
				slide.slideUp();
				slide.addClass('slideHid');
			}
		})
		$('#search').click(function(event){
			event.stopPropagation();
			$('.top_nav_c').slideDown();
			$('.top_nav_c').find('.search #prevName').focus();
			hideSearch();
		});
	})
	function hideSearch(){
		$('.inp_box').click(function(event){
			event.stopPropagation();
		})
		$('body').click(function(event){
			event.stopPropagation();
			$('.top_nav_c').slideUp();
		})
	}
	
</script>
